{
    "_metadata": {
        "major_version": 1,
        "minor_version": 1
    },
    "display_information": {
        "name": "K_RequestBuy"
    },
    "features": {
        "app_home": {
            "home_tab_enabled": true,
            "messages_tab_enabled": false,
            "messages_tab_read_only_enabled": true
        },
        "bot_user": {
            "display_name": "K_RequestBuy",
            "always_online": true
        },
        "shortcuts": [
            {
                "name": "リクエストバイ",
                "type": "global",
                "callback_id": "requestbuy_shortcut_id",
                "description": "KASSEN購入依頼"
            }
        ]
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "groups:history",
                "chat:write",
                "chat:write.public",
                "channels:read",
                "channels:history",
                "users:read",
                "users.profile:read",
                "users:read.email",
                "files:read",
                "files:write",
                "commands"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "app_home_opened",
                "message.channels"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}
